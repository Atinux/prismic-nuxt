{
		"name": "@nuxtjs/prismic",
		"version": "3.0.0",
		"description": "Easily connect your Nuxt.js application to your content hosted on Prismic",
		"keywords": [
				"nuxt",
				"module",
				"nuxt-module",
				"typescript",
				"prismic",
				"cms"
		],
		"repository": {
				"type": "git",
				"url": "ssh://git@github.com/nuxt-community/prismic-module.git"
		},
		"license": "MIT",
		"author": "Nuxt.js Community",
		"type": "module",
		"exports": {
				".": {
						"require": "./dist/index.cjs",
						"import": "./dist/index.js"
				},
				"./package.json": "./package.json"
		},
		"main": "dist/index.cjs",
		"module": "dist/index.js",
		"types": "dist/index.d.ts",
		"files": [
				"dist",
				"src",
				"templates"
		],
		"scripts": {
				"build": "siroc build",
				"dev": "siroc build --watch",
				"format": "prettier --write .",
				"lint": "eslint --ext .js,.ts .",
				"playground:build": "nuxi build playground",
				"playground:dev": "nuxi dev playground",
				"prepare": "npm run build",
				"release": "npm run build && npm run test && standard-version && git push --follow-tags && npm run build && npm publish",
				"release:alpha": "npm run build && npm run test && standard-version --release-as major --prerelease alpha && git push --follow-tags && npm run build && npm publish --tag alpha",
				"release:alpha:dry": "standard-version --release-as major --prerelease alpha --dry-run",
				"release:dry": "standard-version --dry-run",
				"test": "npm run lint && npm run unit",
				"unit": "nyc --reporter=lcovonly --reporter=text --exclude-after-remap=false ava"
		},
		"dependencies": {
				"@nuxt/kit": "0.8.1-edge",
				"@prismicio/vue": "^3.0.0-beta"
		},
		"devDependencies": {
				"@typescript-eslint/eslint-plugin": "^5.5.0",
				"@typescript-eslint/parser": "^5.5.0",
				"ava": "^3.15.0",
				"eslint": "^8.4.0",
				"eslint-config-prettier": "^8.3.0",
				"eslint-plugin-prettier": "^4.0.0",
				"eslint-plugin-tsdoc": "^0.2.14",
				"nuxt3": "latest",
				"nyc": "^15.1.0",
				"prettier": "^2.5.1",
				"prettier-plugin-jsdoc": "^0.3.30",
				"siroc": "^0.16.0",
				"standard-version": "^9.3.2",
				"ts-eager": "^2.0.2",
				"typescript": "^4.5.2"
		},
		"engines": {
				"node": ">=12.7.0"
		},
		"publishConfig": {
				"access": "public"
		}
}
